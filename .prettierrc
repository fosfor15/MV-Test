{
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "auto"
}
